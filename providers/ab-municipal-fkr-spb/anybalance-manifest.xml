<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id version="8">ab-municipal-fkr-spb</id>
	<api flags="no_browser"/>
	<name>НО ФКР МКД СПб (Санкт-Петербург)</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о балансе провайдера Некоммерческая организация Фонд – региональный оператор капитального ремонта общего имущества в многоквартирных домах г. Санкт-Петербург (<a href="https://fkr-spb.ru/">https://fkr-spb.ru/</a>).</p>
	<p>Информация берется из личного кабинета <a href="https://fkr-spb.ru/user">https://fkr-spb.ru//user</a>.</p>
	]]></description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<depends>
		<module id="library"/>
		<module id="misc/saved-data"/>
	</depends>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" ₽"/>
		<counter id="penalty" name="Начислено пени" units=" ₽"/>
		<counter id="account" name="Лицевой счет" type="text"/>
		<counter id="contribution" name="Собираемость взносов по дому" units="%"/>
		<counter id="collectPercentPeny" name="Собираемость пени по дому" units="%"/>
		<counter id="houseKadNum" name="Кадастровый номер дома" type="text"/>
		<counter id="flatKadNum" name="Кадастровый номер квартиры" type="text"/>
		<counter id="adress" name="Адрес" type="text"/>
		<counter id="flatArea" name="Площадь квартиры" units=" м²"/>
		<counter id="fio" name="ФИО" type="text"/>
	</counters>
	<keywords>ЖКХ, ФКР, фонд, Спб, Санкт-Петербург</keywords>
	<type>municipal</type>
	<country>ru</country>
</provider>